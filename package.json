{
  "name": "katy-decorah",
  "version": "0.0.0",
  "description": "Katy DeCorah's personal website",
  "type": "module",
  "scripts": {
    "test": "jest && npm run lint-md && npm run test-content",
    "test-content": "extra-eyes {adventures,code,epicurean,notes}/_posts --dict=katydecorah.dic -q",
    "lint-md": "remark {adventures,code,epicurean,notes}/_posts -q",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "pretty-quick": "pretty-quick",
    "prepare": "husky install",
    "start": "bundle exec jekyll serve",
    "start:prod": "JEKYLL_ENV=production bundle exec jekyll serve"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@double-great/remark-lint-alt-text": "^1.0.0",
    "@mapbox/remark-lint-link-text": "^0.6.0",
    "doctoc": "^2.2.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "extra-eyes": "^3.2.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "remark-cli": "^11.0.0",
    "remark-frontmatter": "^4.0.1",
    "remark-frontmatter-validator": "^0.4.0",
    "remark-lint-haiku": "^0.2.0",
    "remark-lint-heading-increment": "^3.1.1"
  },
  "lint-staged": {
    "katydecorah.dic": [
      "./bin/alpha"
    ],
    "code/_posts/2021-06-05-lint-markdown.md": [
      "doctoc"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,html,json,yml,cjs}": "prettier --write"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/katydecorah.github.io.git"
  },
  "author": "Katy DeCorah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katydecorah/katydecorah.github.io/issues"
  },
  "homepage": "https://github.com/katydecorah/katydecorah.github.io#readme"
}
